{
  "name": "matperez/yii2-platron",
  "description": "Yii2 Platron payment gateway API",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "payment",
    "billing",
    "gateway"
  ],
  "license": "MIT",
  "require": {
    "php":">=5.5",
    "yiisoft/yii2": "~2.0",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "mockery/mockery": "^0.9.5"
  },
  "autoload": {
    "psr-4": {
      "matperez\\yii2platron\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "matperez\\yii2platron\\tests\\": "./tests"
    }
  }
}
